package view;

import java.awt.Font;

import javax.swing.JLabel;

public class headingLabel extends JLabel {
	
	private Font bold = new Font(this.getFont().getFontName(), Font.BOLD, this.getFont().getSize());

	public headingLabel(String string) {
		//bold = new Font(this.getFont().getFontName(), Font.BOLD, this.getFont().getSize());
		setText(string);
	}

}
