package view;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

import javax.swing.JButton;
import javax.swing.JPanel;

public class MainDisplay extends JPanel{

	
	List<String> events = Arrays.asList("Add Employee","Delete employee","Update Employee",
			"View Employee","Post timecard","post receipt","post union service charge","run payroll");
	
	public MainDisplay() {
		

		for (String element : events) {
			JButton btn = new JButton();
			btn.setActionCommand(element);
			btn.setText(element);
			this.add(btn);
			
			
		}
	}

}
